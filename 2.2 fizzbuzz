"""
fizzbuzz

"""

print("Welcome to fizzbuzz game")
users_number = input("Pick a number between 1 and 100: ")
users_number = int(users_number)

for number in range(1, users_number+1):
    if ((number % 3 == 0) & (number % 5 == 0)):
        print("fizzbuzz")
    elif number % 3 == 0:
        print("fizz")
    elif number % 5 == 0:
        print("buzz")
    else:
        print(number)
